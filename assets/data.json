			"background-tint": {
				"name": "Tint the Background Image",
				"css": {"bottom": "/*-S-T-A-R-T--------------------*\\\n| Background Tint                |\n\\*------------------------------*/\n\nbody::before {\n\t/* change colour here */\n\tbackground: rgba(0, 0, 0, 0.8);\n\t\n\tcontent: \"\";\n\tz-index: -1;\n\tposition: fixed;\n\ttop: 0;\n\tleft: 0;\n\twidth: 100%;\n\theight: 100%;\n}\n\n/*------------------------E-N-D-*/"},
				"options": {
					"background-tint-c": {
						"name": "Tint Colour",
						"type": "colour",
						"replacements": [["rgba(0, 0, 0, 0.8)", "{{{insert}}}"]],
						"default": "rgba(0, 0, 0, 0.8)"
					}
				}
			},
			"cat-banner": {
				"name": "Per-category Banner Images",
				"css": {"bottom": "/*-S-T-A-R-T--------------------*\\\n| Per-Category Banner Image      |\n\\*------------------------------*/\n\n/* All Anime */\n[data-query*='status\":7'] { --banner: url(URLHERE); }\n/* Watching/Reading */\n[data-query*='status\":1'] { --banner: url(URLHERE); }\n/* Completed */\n[data-query*='status\":2'] { --banner: url(URLHERE); }\n/* Onhold */\n[data-query*='status\":3'] { --banner: url(URLHERE); }\n/* Dropped */\n[data-query*='status\":4'] { --banner: url(URLHERE); }\n/* Planned */\n[data-query*='status\":6'] { --banner: url(URLHERE); }\n\n/*------------------------E-N-D-*/"},
				"options": {
					"cat-banner-7": {
						"name": "All Anime/Manga Image",
						"type": "image_url",
						"replacements": [["7'] { --banner: url(URLHERE)", "7'] { --banner: {{{insert}}}"]]
					},
					"cat-banner-1": {
						"name": "Watching/Reading Image",
						"type": "image_url",
						"replacements": [["1'] { --banner: url(URLHERE)", "7'] { --banner: {{{insert}}}"]]
					},
					"cat-banner-2": {
						"name": "Completed Image",
						"type": "image_url",
						"replacements": [["2'] { --banner: url(URLHERE)", "7'] { --banner: {{{insert}}}"]]
					},
					"cat-banner-3": {
						"name": "On Hold Image",
						"type": "image_url",
						"replacements": [["3'] { --banner: url(URLHERE)", "7'] { --banner: {{{insert}}}"]]
					},
					"cat-banner-4": {
						"name": "Dropped Image",
						"type": "image_url",
						"replacements": [["4'] { --banner: url(URLHERE)", "7'] { --banner: {{{insert}}}"]]
					},
					"cat-banner-6": {
						"name": "Plan to Watch/Read Image",
						"type": "image_url",
						"replacements": [["6'] { --banner: url(URLHERE)", "7'] { --banner: {{{insert}}}"]]
					}
				}
			},
			"cat-character": {
				"name": "Per-category Character Images",
				"css": {"bottom": "/*-S-T-A-R-T--------------------*\\\n| Per-Category Character Image   |\n\\*------------------------------*/\n\n/* All Anime */\n[data-query*='status\":7'] { --character: url(URLHERE); }\n/* Watching/Reading */\n[data-query*='status\":1'] { --character: url(URLHERE); }\n/* Completed */\n[data-query*='status\":2'] { --character: url(URLHERE); }\n/* Onhold */\n[data-query*='status\":3'] { --character: url(URLHERE); }\n/* Dropped */\n[data-query*='status\":4'] { --character: url(URLHERE); }\n/* Planned */\n[data-query*='status\":6'] { --character: url(URLHERE); }\n\n/*------------------------E-N-D-*/"},
				"options": {
					"cat-character-7": {
						"name": "All Anime/Manga Image",
						"type": "image_url",
						"replacements": [["7'] { --character: url(URLHERE)", "7'] { --character: {{{insert}}}"]]
					},
					"cat-character-1": {
						"name": "Watching/Reading Image",
						"type": "image_url",
						"replacements": [["1'] { --character: url(URLHERE)", "7'] { --character: {{{insert}}}"]]
					},
					"cat-character-2": {
						"name": "Completed Image",
						"type": "image_url",
						"replacements": [["2'] { --character: url(URLHERE)", "7'] { --character: {{{insert}}}"]]
					},
					"cat-character-3": {
						"name": "On Hold Image",
						"type": "image_url",
						"replacements": [["3'] { --character: url(URLHERE)", "7'] { --character: {{{insert}}}"]]
					},
					"cat-character-4": {
						"name": "Dropped Image",
						"type": "image_url",
						"replacements": [["4'] { --character: url(URLHERE)", "7'] { --character: {{{insert}}}"]]
					},
					"cat-character-6": {
						"name": "Plan to Watch/Read Image",
						"type": "image_url",
						"replacements": [["6'] { --character: url(URLHERE)", "7'] { --character: {{{insert}}}"]]
					}
				}
			},
			"cat-background": {
				"name": "Per-category Background Images",
				"css": {"bottom": "/*-S-T-A-R-T--------------------*\\\n| Per-Category Background Image  |\n\\*------------------------------*/\n\n/* All Anime */\nbody[data-query*='status\":7'] { --background: url(URLHERE); }\n/* Watching/Reading */\nbody[data-query*='status\":1'] { --background: url(URLHERE); }\n/* Completed */\nbody[data-query*='status\":2'] { --background: url(URLHERE); }\n/* Onhold */\nbody[data-query*='status\":3'] { --background: url(URLHERE); }\n/* Dropped */\nbody[data-query*='status\":4'] { --background: url(URLHERE); }\n/* Planned */\nbody[data-query*='status\":6'] { --background: url(URLHERE); }\n\n/*------------------------E-N-D-*/"},
				"options": {
